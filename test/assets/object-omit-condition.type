/* adapted from https://github.com/type-challenges/type-challenges/blob/master/questions/4-easy-pick/README.md */

type Todo = {
  title: string,
  description: string,
  completed: boolean
}

type TodoPreview = ``` "use js" 
  return $.use("Todo")
    .omit(key => key.startsWith("t") || key.startsWith("d"))
    .type();
```